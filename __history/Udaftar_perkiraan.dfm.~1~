  object Qdaftar_perkiraan: TUniQuery
    Connection = dm.Koneksi
    SQL.Strings = (
      
        'select a.*, b.nama_header,c.nama_posting,d.nm_kategori,e.jenis_a' +
        'kun,case when kelompok_akun = '#39'1'#39' then '#39'NERACA'#39' ELSE '#39'L/R'#39' END K' +
        'elompok from t_daftar_perkiraan a'
      'left join t_header_perkiraan b on a.kode_header=b.kode_header'
      'left join t_posting c on a.posting=c.id '
      
        'left join t_kategori_akun d on a.kd_kategori_akun=d.kode_kategor' +
        'i'
      'left join t_jenis_akun e on a.id_jenis_akun=e.id'
      'order by kode ASC')
    Left = 240
    Top = 64
  end
  object Dsdaftar_perkiraan: TDataSource
    DataSet = MemTableEh1
    Left = 427
    Top = 40
  end
  object MemTableEh1: TMemTableEh
    FetchAllOnOpen = True
    Params = <>
    DataDriver = DataSetDriverEh1
    Left = 416
    Top = 96
  end
  object DataSetDriverEh1: TDataSetDriverEh
    ProviderDataSet = Qdaftar_perkiraan
    Left = 608
    Top = 368
  end
  object DSdetail_perkiraan: TDataSource
    DataSet = Qdetail_perkiraan
    Left = 344
    Top = 248
  end
  object Qdetail_perkiraan: TUniQuery
    Connection = dm.Koneksi
    SQL.Strings = (
      'select a.*,b.nama_modul from t_daftar_perkiraan_detail a'
      'left join t_modul b on a.id_modul=b.id')
    MasterSource = Dsdaftar_perkiraan
    MasterFields = 'kode'
    DetailFields = 'kode_perkiraan'
    Left = 454
    Top = 226
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'kode'
        Value = nil
      end>
  end
