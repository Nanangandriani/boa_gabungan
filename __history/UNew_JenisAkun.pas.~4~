unit UNew_JenisAkun;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.StdCtrls, RzBtnEdt, Vcl.Mask,
  RzEdit, RzButton, Vcl.ExtCtrls;

type
  TFNew_JenisAkun = class(TForm)
    Panel1: TPanel;
    BBatal: TRzBitBtn;
    BSimpan: TRzBitBtn;
    EdKategori: TEdit;
    Label2: TLabel;
    Label4: TLabel;
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  FNew_JenisAkun: TFNew_JenisAkun;

implementation

{$R *.dfm}

var
//  FPakai_BahanPersbu: TFPakai_BahanPersbu;
  RealFNewPosting: TFNewPosting;

function FNewPosting: TFNewPosting;
begin
  if RealFNewPosting <> nil then
    FNewPosting:= RealFNewPosting
  else
    Application.CreateForm(TFNewPosting, Result);
end;


end.
