{$R *.dfm}

uses UNew_Gudang;

procedure TFListGudang.dxBarLargeBaruClick(Sender: TObject);
begin
  FNew_Gudang.Show;
  with FNew_gudang do
  begin
    CLear;
    Fnew_gudang.show;
    FNew_gudang.BSimpan.Visible:=true;
    FNew_gudang.BEdit.Visible:=false;
    FNew_gudang.Ednm.SetFocus;
    Caption:='New Gudang';
    Edkd.Enabled:=True;
  end;

end;

end.
