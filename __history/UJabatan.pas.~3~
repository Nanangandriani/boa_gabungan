{$R *.dfm}

uses UNew_Jabatan;

procedure TFJabatan.ActBaruExecute(Sender: TObject);
begin
    with Fnew_jabatan do
    begin
      EDkd.clear;
      EdDept.Clear;
      Autonumber;
      Show;
      status:=0;
    end;
end;
end;

