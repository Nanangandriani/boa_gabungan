  object QRptKontrak: TUniQuery
    Connection = dm.Koneksi
    SQL.Strings = (
      
        'SELECT'#9'c.item_name,a.detail_id,a.contract_no,a.item_stock_code,a' +
        '.qty,a.price,a.unit,a.total_price,a.remaining_qty,a.totalpo,'
      
        'a.status,a.specification,a.grandtotal,a.ppn_rp,a.ppn,sum(a.price' +
        '+((a.price/100)* a.ppn)) AS price2,d.supplier_code,d.contract_no' +
        ','
      
        'd.finish_date,d.due_date,d.order_no,d.trans_year,d.remarks,d.sta' +
        'tus,d.currency,d.currency_value,d.approval,d.pic,e.supplier_name' +
        ','
      
        'e.address,e.contact_person,d.delivery_month,d.delivery_year,a.su' +
        'btotal_rp,d.contract_date'
      'FROM'
      
        't_coop_contract_det AS "a"'#9'INNER JOIN'#9't_item_stock AS b'#9'ON '#9'a.it' +
        'em_stock_code = b.item_stock_code'#9'INNER JOIN'#9't_item AS "c"'
      
        'ON '#9#9'b.item_code = c.item_code INNER JOIN'#9't_coop_contract AS d'#9'O' +
        'N a.contract_no = d.contract_no'#9'INNER JOIN'#9't_supplier AS e'#9'ON d.' +
        'supplier_code = e.supplier_code '
      
        'GROUP BY '#9'c.item_name,a.detail_id,a.contract_no,a.item_stock_cod' +
        'e,a.qty,a.price,a.unit,a.total_price,a.remaining_qty,a.totalpo,'
      
        'a.status,a.specification,a.grandtotal,a.ppn_rp,a.ppn,d.supplier_' +
        'code,d.contract_no,d.finish_date,d.due_date,d.order_no,d.trans_y' +
        'ear,'
      
        'd.remarks,d.status,d.currency,d.currency_value,d.approval,d.pic,' +
        'e.supplier_name,e.address,e.contact_person,d.delivery_month,'
      'd.delivery_year,a.subtotal_rp,d.contract_date')
    Left = 480
    Top = 88
  end
