  object Q_list_so: TUniQuery
    Connection = dm.Koneksi
    SQL.Strings = (
      
        '--SELECT notrans,vehicle_group_sort_number,vehicle_group_id,vehi' +
        'cles from t_sales_order '
      '--WHERE vehicle_group_sort_number is NOT NULL '
      ''
      ''
      
        'select DISTINCT type_vehicles_code,vehicle_group_sort_number,veh' +
        'icles,type_vehicles_name,capacity,sent_date,deleted_at FROM'
      
        '(SELECT notrans,order_date,sent_date,vehicle_group_sort_number,v' +
        'ehicle_group_id,vehicles,type_vehicles_code,type_vehicles_name,c' +
        'apacity,deleted_at from t_sales_order WHERE vehicle_group_sort_n' +
        'umber is NOT NULL)a WHERE sent_date='#39'2025-10-08'#39' and deleted_at ' +
        'IS NULL'
      'ORDER BY type_vehicles_code ASC')
